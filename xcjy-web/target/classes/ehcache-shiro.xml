<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<ehcache name="shiroCache" updateCheck="false">

    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            />
            
</ehcache>
